histpuntajes <- function(puntajes) {
h <- hist(puntajes, plot = FALSE)
plot_ly(x = ~puntajes, type = "histogram", nbinsx = 2*length(h$counts), marker = list(color="lightskyblue1",line = list(color = "black", width = 2)))
}
